{
  "name": "stremio-porn",
  "version": "0.0.1",
  "description": "",
  "author": "Naughty Doge <naughty-doge@protonmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --inspect dist/index.js",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "rimraf dist/**/* && babel src -d dist --source-maps",
    "test-unit": "jest --testNamePattern=\"^(?!.*@integration).*$\"",
    "test-integration": "jest --testNamePattern=\"^.*@integration.*$\"",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.11.5"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-object-rest-spread",
      [
        "@babel/plugin-proposal-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.40",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "eslint": "^4.18.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cache-manager": "^2.9.0",
    "chalk": "^2.4.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.4",
    "got": "^8.3.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "serve-static": "^1.13.2",
    "stremio-addons": "^2.8.14",
    "xml-js": "^1.6.2"
  }
}
