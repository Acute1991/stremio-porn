{
  "name": "stremio-porn",
  "version": "0.0.1",
  "description": "",
  "author": "Naughty Doge <naughty-doge@protonmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node dist/index.js",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "rimraf dist/**/* && babel src -d dist --source-maps",
    "test": "jest --testNamePattern=\"^(?!.*@integration).*$\"",
    "test-all": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.11.5"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "syntax-object-rest-spread",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cross-env": "^5.1.4",
    "got": "^8.3.0",
    "stremio-addons": "^2.8.14"
  }
}
